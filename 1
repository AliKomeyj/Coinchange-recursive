#include <iostream>
#include <vector>
using namespace std;
int coinchange(vector<int>, int, int);
int main()
{
    vector<int> coins = {1, 2, 5, 10, 50};
    int sum = 0, n = 0;
    cin >> n;
    cout << coinchange(coins, coins.size(), n);
}
int coinchange(vector<int> coins, int size, int n)
{
    if (n == 0)
        return 1;
    if (n < 0)
        return 0;
    if (size <= 0 && n >= 1)
        return 0;
    return coinchange(coins, size - 1, n) + coinchange(coins, size, n - coins[size - 1]);
}
